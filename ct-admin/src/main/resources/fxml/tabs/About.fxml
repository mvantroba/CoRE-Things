<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<HBox styleClass="container-sub" xmlns="http://javafx.com/javafx/8.0.65"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox HBox.hgrow="ALWAYS">
			<children>
				<StackPane>
					<children>
						<Label styleClass="card-heading" text="%card.title.about"
							StackPane.alignment="CENTER_LEFT" />
					</children>
					<VBox.margin>
						<Insets bottom="8.0" />
					</VBox.margin>
				</StackPane>
				<Label text="%about" wrapText="true">
					<VBox.margin>
						<Insets top="16.0" />
					</VBox.margin>
				</Label>
				<Label text="%about.supervisors" wrapText="true">
					<VBox.margin>
						<Insets top="16.0" />
					</VBox.margin>
				</Label>
			</children>
			<styleClass>
				<String fx:value="card" />
				<String fx:value="card-content" />
			</styleClass>
			<HBox.margin>
				<Insets bottom="64.0" left="256.0" right="256.0" />
			</HBox.margin>
		</VBox>
	</children>
</HBox>
